{
  "name": "flight-account-menu",
  "version": "0.1.0",
  "description": "Alces Flight Platform account menu",
  "author": "Alces Flight Ltd",
  "license": "EPL",
  "repository": " openflighthpc/flight-account-menu",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs,umd",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "ouch": "microbundle-crl --no-compress --format umd --external React,react-is,react-dom,seamless-immutable,redux-axios-middleware --globals react=React",
    "pouch": "microbundle-crl --no-compress --format modern --external react,react-is,react-dom,seamless-immutable,redux-axios-middleware --globals react=React",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "lint": "eslint src/"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "js-gravatar": "^1.1.3",
    "jwt-decode": "^2.2.0",
    "pluralize": "^8.0.0",
    "react-hook-form": "^6.7.0",
    "react-redux": "^7.2.1",
    "reactstrap": "^8.5.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "universal-cookie": "^4.0.3",
    "validator": "^13.1.1"
  },
  "resolutions": {
    "@rollup/plugin-commonjs": "11.0.2"
  }
}
